{
    "manifest_version": 2,
    "name": "System Indicator",
    "version": "0.1",
    "content_scripts": [
        {
        "matches": [
            "<all_urls>"
        ],
        "js": ["src/js/script.js"],
        "css": ["src/css/style.css"]
      }
    ],
    "browser_action": {
        "default_popup": "src/index.html",
        "default_title": "System Indicator"
    }
}